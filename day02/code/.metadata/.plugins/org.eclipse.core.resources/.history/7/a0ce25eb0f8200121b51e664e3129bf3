package com.itheima.service;

import android.util.Log;

public class CalcService {
	private static final String tag = "CalcService";
	/**
	 * 相加的业务方法
	 * @param x 第一个int
	 * @param y 第二个int
	 * @return
	 */
	public int add(int x ,int y){
		Log.v(tag, "x="+x);
		Log.d(tag, "y="+y);
		int result = x+y;
		Log.i(tag,"result="+result);
		
		return x+y;
	}

	
	
}
